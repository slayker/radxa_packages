<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Mobile network settings, network mode choices for a device supporting GSM and LTE [CHAR LIMIT=25] -->
    <string-array name="preferred_network_mode_choices_lte_gsm">
        <item>LTE/WCDMA</item>
        <item>LTE/GSM/WCDMA</item>
        <item>GSM/WCDMA preferred</item>
    </string-array>
    <string-array name="preferred_network_mode_values_lte_gsm" translatable="false">
        <item>"12"</item>
        <item>"9"</item>
        <item>"0"</item>
    </string-array>

    <!-- Values for the flip action settings. -->
    <string-array name="flip_action_setting_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Entries listed in the setting for the flip action. -->
    <string-array name="flip_action_setting_entries">
        <item>Do nothing</item>
        <item>Mute ringer</item>
        <item>Dismiss call</item>
    </string-array>

    <!-- Summary texts for flip action. Must be ordered by numerical
         value (do nothing = 0, mute = 1, dismiss = 2) -->
    <string-array name="flip_action_summary_entries">
        <item>do nothing</item>
        <item>mute the ringer</item>
        <item>dismiss the call</item>
    </string-array>

    <!-- Summary texts for Voice Quality. -->
    <string-array translatable="false" name="voice_quality_entries">
        <!-- Overlay appropriately, using string constants as above. E.g.:
        <item>@string/voice_quality_normal</item>
        <item>@string/voice_quality_clear</item>  -->
    </string-array>
    <!-- Voice Quality values passed to AudioSystem. DO NOT TRANSLATE. -->
    <string-array translatable="false" name="voice_quality_values">
        <!-- Overlay appropriately -->
    </string-array>

    <!-- The status the phone app writes to a proximity sysfs node on "answer call" and "hangup".
         First item: value for "answer call"
         Second item: value for "hangup"  -->
    <string-array name="phone_app_status_values" translatable="false">
        <!-- Example:
        <item>"2"</item>
        <item>"1"</item>  -->
    </string-array>

</resources>
